{
  "expo": {
    "name": "trips-nd-fare",
    "slug": "trips-nd-fare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "clikride",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/splash.png",
      "contentFit": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.webiq.tripsndfare",
      "googleServicesFile": "./src/config/GoogleService-Info.plist",
      "infoPlist": {
        "CFBundleURLTypes": [
            {
              "CFBundleURLSchemes": ["com.googleusercontent.apps.562192654841-fvfpnk2usg9h53170quig4jh2r5luegr"]
            }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyADwNi9utUXmZA6jCPGRf1HVjjvb5PL42E"
        }
      },
      "package": "com.webiq.tripsndfare",
      "package_name": "com.webiq.tripsndfare",
      "googleServicesFile": "./src/config/google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos so that you can update your profile."
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "$(PRODUCT_NAME) would like to use your microphone for voice recording."
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8bb616cb-61a4-458e-8613-f652992560f5"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8bb616cb-61a4-458e-8613-f652992560f5"
    }
  }
